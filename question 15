CODE
#include<stdio.h>
int main()
{
    int n,i,j,b=0,s=0;
    printf("Enter the dimension of the array");
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
      {
          printf("Enter%d element",i+1);
          scanf("%d",&a[i]);
      }
    for(i=0;i<n & b<n/2;i++)
    {
      b=0;
      for(j=0;j<n;j++)
        if(a[i]==a[j])
         b++;
    }
    if(b>n/2)
     printf("%d",a[i-1]);
    else
    printf("No major element");
                return 0;
}
